 !(bind! &space (new-space))
 !(load-ascii &space data_subset/gencode/edges.metta)
 !(load-ascii &space data_subset//gencode/nodes.metta)
 !(load-ascii &space data_subset/gaf/edges.metta)
 !(load-ascii &space data_subset/gtex/eqtl/edges.metta)
 !(load-ascii &space data_subset/ontology/edges.metta)
 !(load-ascii &space data_subset/ontology/nodes.metta)
 !(load-ascii &space data_subset/reactome/edges.metta)
 !(load-ascii &space data_subset/reactome/nodes.metta)
